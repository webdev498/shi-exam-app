    "rxjs": "5.0.3",
    "zone.js": "~0.7.4"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.28",
    "@types/node": "^4.0.30",
    "assets-webpack-plugin": "^3.4.0",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "copy-webpack-plugin": "^3.0.1",
