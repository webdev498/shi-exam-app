{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cgi-she-exam-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "prefix": "app",
      "styles": [
        "styles/account.scss",
        "styles/app.scss",
        "styles/categories.scss",
        "styles/exam.scss",
        "styles/examcomplete.scss",
        "styles/examhistory.scss",
        "styles/examstart.scss",
        "styles/feedback.scss",
        "styles/fillinblank.scss",
        "styles/flashcard.scss",
        "styles/forgotpassword.scss",
        "styles/grouping.scss",
        "styles/home.scss",
        "styles/login.scss",
        "styles/matching.scss",
        "styles/multiplechoice.scss",
        "styles/premiumupgrade.scss",
        "styles/register.scss",
        "styles/selection.scss",
        "styles/study.scss",
        "styles/studyquestionchoice.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "src/**/*.ts",
      "project": "src/tsconfig.json"
    },
    {
      "files": "e2e/**/*.ts",
      "project": "e2e/tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
